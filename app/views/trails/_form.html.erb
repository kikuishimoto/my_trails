
<%= form_for @trail do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <br>
    <%= f.label :length %>
    <%= f.text_field :length %>
    <br>
    <%= f.label :skill_level %>
    <%= f.text_field :skill_level %>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br>
    <%= f.submit "Submit" %>
<% end %>
<h1> Welcome to Every Trail! </h1>
<h3> Click on a trail to explore! </h3>
<h4> Know a trail we dont? <%= link_to "Create a trail!", new_trail_path(@trail) %> </h4>
<% @trails.each do |t| %>
    <%= link_to t.name, trail_path(t) %>
    <%= t.city %>
    <%= t.length %> 
    <%= t.skill_level %>
    <%= t.description %>
<%end%>
